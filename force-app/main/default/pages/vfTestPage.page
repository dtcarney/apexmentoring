
<apex:page standardController="Feedback__c" lightningStylesheets="true">
    <apex:form>
        <apex:pageBlock title="Feedback Details">
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!Feedback__c.Training__c}"/>
                <apex:inputField value="{!Feedback__c.Participant__c}"/>
                <apex:inputField value="{!Feedback__c.Comment__c}"/>
                <apex:inputField value="{!Feedback__c.Rating__c}"/>
            </apex:pageBlockSection>
            <apex:commandButton value='Save' action="{!save}"/>
        </apex:pageBlock>
    </apex:form>
</apex:page>