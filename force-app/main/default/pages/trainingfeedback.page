
<apex:page controller="TrainingFeedbackController" lightningStylesheets="true">
    <apex:form>
        <apex:pageBlock title="Feedback Details">
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!feedback.Training__c}"/>
                <apex:inputField value="{!feedback.Participant__c}"/>
                <apex:inputField value="{!feedback.Comment__c}"/>
                <apex:inputField value="{!feedback.Rating__c}"/>
            </apex:pageBlockSection>
            <apex:commandButton value='Save' action="{!save}"/>
        </apex:pageBlock>
    </apex:form>
</apex:page>